{
    "name": "wyrihaximus/github-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "wyrihaximus/api-client": "dev-master"
    },
    "require-dev": {
        "wyrihaximus/travis-client": "dev-master",
        "phpunit/phpunit": "^5.2.3",
        "squizlabs/php_codesniffer": "^2.6",
        "vectorface/dunit": "~2.0",
        "wyrihaximus/api-client-resource-generator": "dev-master",
        "phake/phake": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\Github\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Tests\\Github\\": "tests/"
        }
    },
    "suggest": {
        "wyrihaximus/appveyor-client": "Easy access to AppVeyor client",
        "wyrihaximus/scrutinizer-client": "Easy access to Scrutinizer client",
        "wyrihaximus/styleci-client": "Easy access to StyleCI client",
        "wyrihaximus/travis-client": "Easy access to Travis client"
    }
}
