    "require": {
        "php": "^7.0",
        "api-clients/foundation": "dev-master",
        "wyrihaximus/api-client": "dev-master"
    },
    "require-dev": {
        "wyrihaximus/travis-client": "dev-master",
        "phpunit/phpunit": "^5.2.3",
        "squizlabs/php_codesniffer": "^2.6",
        "vectorface/dunit": "~2.0",
        "api-clients/resource-generator": "dev-master",
        "phake/phake": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "ApiClients\\Github\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiClients\\Tests\\Github\\": "tests/"
        }
    },
    "suggest": {
        "api-clients/appveyor": "Easy access to AppVeyor client",
        "api-clients/scrutinizer": "Easy access to Scrutinizer client",
        "api-clients/styleci": "Easy access to StyleCI client",
        "api-clients/travis": "Easy access to Travis client"
